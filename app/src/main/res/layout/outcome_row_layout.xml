<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="75dp">

    <TextView
        android:layout_width="30dp"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:text="@string/textView"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvNo"
        />
    <TextView
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:text="@string/textView"
        android:layout_toEndOf="@+id/tvNo"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvOutcomeTypeId"
        />
    <TextView
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:text="@string/textView"
        android:layout_toEndOf="@+id/tvOutcomeTypeId"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvOutcomeDate"
        />
    <TextView
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:text="@string/textView"
        android:layout_toEndOf="@+id/tvOutcomeDate"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvDay"
        />
    <TextView
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:text="@string/textView"
        android:layout_toEndOf="@+id/tvDay"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvMonth"
        />
    <TextView
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:text="@string/textView"
        android:layout_toEndOf="@+id/tvMonth"
        android:gravity="center"
        android:textAppearance="?android:textAppearanceLarge"
        android:id="@+id/tvYear"
        />
</RelativeLayout>